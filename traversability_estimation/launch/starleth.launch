<launch>
  <env name="ROSCONSOLE_CONFIG_FILE" value="$(find traversability_estimation)/launch/custom_rosconsole.conf"/>
  <node pkg="traversability_estimation" type="traversability_estimation" name="traversability_estimation" output="screen">
    <rosparam command="load" file="$(find traversability_estimation)/config/starleth.yaml" />
    <rosparam command="load" file="$(find traversability_estimation)/config/elevation_map_filter.yaml" />
  </node>
</launch>